<?xml version="1.0" encoding="UTF-8" ?>
<log-oper>
	<logset>
	
		<oper
			id="sysTaskIndexOperLog"
			extends="BaseOperLog"
			model="com.landray.kmss.sys.task.model.SysTaskMain"
			action="/sys/task/sys_task_main/sysTaskIndex.do">
			<query
				method="calendar"
				logPoint="Base_UrlParam;Action_FindAll"
				messageKey="sys-task:sysTaskMain.list.search">
			</query>
			<query
				method="getChildTask"
				logPoint="Base_UrlParam;Action_FindAll"
				messageKey="sys-task:sysTaskIndex.oper.getChildTask">
			</query>
			<query
				method="showKeydataUsed"
				logPoint="Base_UrlParam;Action_FindAll"
				messageKey="sys-task:sysTaskIndex.oper.keydata">
			</query>
			<query
				method="listbyModelName"
				logPoint="Base_UrlParam;Action_FindAll"
				messageKey="sys-task:sysTaskIndex.oper.listbyModelName">
			</query>
		</oper>
		<oper
			id="sysTaskMainOperLog"
			extends="BaseOperLog"
			model="com.landray.kmss.sys.task.model.SysTaskMain"
			action="/sys/task/sys_task_main/sysTaskMain.do">
			<query
				method="copyTask|copyChildTask"
				logPoint="Base_UrlParam;Action_Find"
				messageKey="sys-task:button.copy">
			</query>
			<query
				method="updateAttention"
				logPoint="Base_UrlParam;updateAttention"
				messageKey="sys-task:table.sysTaskAttention">
			</query>
			<query
				method="updateFinish"
				logPoint="Service_Update"
				messageKey="sys-task:sysTaskMain.oper.updateFinish">
			</query>
			<query
				method="updateUnFinish"
				logPoint="Service_Update"
				messageKey="sys-task:sysTaskMain.oper.updateUnFinish">
			</query>
			<query
				method="updateStop"
				logPoint="Service_Update"
				messageKey="sys-task:sysTaskMain.oper.updateStop">
			</query>
			<query
				method="updateCancelStop"
				logPoint="Service_Update"
				messageKey="sys-task:sysTaskMain.oper.updateCancelStop">
			</query>
			<query
				method="closeTask"
				logPoint="Service_Update"
				messageKey="sys-task:button.closeTask">
			</query>
			<query
				method="listByAppionDept|listByPerformDept"
				logPoint="Base_UrlParam;Action_FindAll"
				messageKey="sys-task:sysTaskMain.oper.list">
			</query>
			<query
				method="batchSave"
				logPoint="Service_Add"
				messageKey="sys-task:sysTaskMain.oper.batchSave">
			</query>
			<query
				method="batchUpdate"
				logPoint="Service_Update"
				messageKey="sys-task:sysTaskMain.oper.batchUpdate">
			</query>
			<query
				method="updatePerform"
				logPoint="Service_Update"
				messageKey="sys-task:sysTaskMain.oper.updatePerform">
			</query>
		</oper>
		<oper
			id="sysTaskFeedbackOperLog"
			extends="BaseOperLog"
			model="com.landray.kmss.sys.task.model.SysTaskFeedback"
			action="/sys/task/sys_task_feedback/sysTaskFeedback.do">
			<query
				method="export"
				logPoint="Base_UrlParam;Action_FindAll"
				messageKey="sys-task:button.exportExcel">
			</query>
			<query
				method="loadAttachmentInfo"
				logPoint="Base_UrlParam;Action_Find"
				messageKey="sys-task:sysTaskFeedback.oper.loadAttInfo">
			</query>
		</oper>
		<oper
			id="sysTaskApproveOperLog"
			extends="BaseOperLog"
			model="com.landray.kmss.sys.task.model.SysTaskApprove"
			action="/sys/task/sys_task_approve/sysTaskApprove.do">
			<query
				method="getTaskApprove"
				logPoint="Base_UrlParam;Action_FindAll"
				messageKey="sys-task:sysTaskApprove.oper.getTaskApprove">
			</query>
			<query
				method="updateIsAvailable"
				logPoint="Service_Update">
			</query>
		</oper>
		<oper
			id="sysTaskAnalyzeOperLog"
			extends="BaseOperLog"
			model="com.landray.kmss.sys.task.model.SysTaskAnalyze"
			action="/sys/task/sys_task_analyze/sysTaskAnalyze.do">
			<query
				method="save|saveadd"
				logPoint="Service_Add;Service_Update">
			</query>
			<query
				method="listResult"
				logPoint="Base_UrlParam;Action_FindAll"
				messageKey="sysTaskAnalyze.oper.listResult">
			</query>
			<query
				method="listPriviewResult"
				logPoint="Base_UrlParam;Action_FindAll"
				messageKey="sysTaskAnalyze.oper.listPriviewResult">
			</query>
			<query
	         	method="statChart"
	         	logPoint="Base_UrlParam;statChart">
	         </query>
	         <query
	         	method="exportExcel"
	         	logPoint="Base_UrlParam;exportExcel">
	         </query>
		</oper>
		<oper
			id="sysTaskMainPortletOperLog"
			extends="BaseOperLog"
			model="com.landray.kmss.sys.task.model.SysTaskMain"
			action="/sys/task/sys_task_portlet/SysTaskMainPortlet.do">
			<query
				method="getDataList"
				logPoint="Base_UrlParam;Action_FindAll"
				messageKey="sys-task:sysTaskMainPortlet.oper.getDataList">
			</query>
		</oper>
		<oper
			id="sysTaskPortletOperLog"
			extends="BaseOperLog"
			model="*"
			action="/sys/common/dataxml.jsp">
			<query
            	paramName="s_bean"
            	paramValue="sysTaskMainPortletService"
            	logPoint="Base_UrlParam;sysTaskMainPortletService"
            	messageKey="sys-task:sysTaskMain.portlet.list">
            </query>
		</oper>
		<oper
        	id="sysTaskCategoryOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.sys.task.model.SysTaskCategory"
        	action="/sys/task/sys_task_category/sysTaskCategory.do">
        	<query
        		method="updateIsAvailable"
        		logPoint="Service_Update">
        	</query>
        </oper>
	</logset>
</log-oper>