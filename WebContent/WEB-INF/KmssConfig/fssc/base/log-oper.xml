<?xml version="1.0" encoding="UTF-8" ?>
<!-- 财务基础数据模块默认监听增删改查 -->
<log-oper>
	<logset>
		<!-- 员工账户 -->
        <oper
        	id="fsscBaseAccountOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseAccount"
        	action="/fssc/base/fssc_base_account/fsscBaseAccount.do">
        </oper>
		<!-- 会计科目(公共) -->
        <oper
        	id="fsscBaseAccountsOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseAccounts"
        	action="/fssc/base/fssc_base_accounts/fsscBaseAccounts.do">
        </oper>
		<!-- 会计科目(公司) -->
        <oper
        	id="fsscBaseAccountsComOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseAccountsCom"
        	action="/fssc/base/fssc_base_accounts_com/fsscBaseAccountsCom.do">
        </oper>
		<!-- 地域 -->
        <oper
        	id="fsscBaseAreaOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseArea"
        	action="/fssc/base/fssc_base_area/fsscBaseArea.do">
        </oper>
        <!-- 舱位 -->
        <oper
        	id="fsscBaseBerthOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseBerth"
        	action="/fssc/base/fssc_base_berth/fsscBaseBerth.do">
        </oper>
        <!-- 预算科目(公共) -->
        <oper
        	id="fsscBaseBudgetItemOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseBudgetItem"
        	action="/fssc/base/fssc_base_budget_item/fsscBaseBudgetItem.do">
        </oper>
        <!-- 预算科目(公司) -->
        <oper
        	id="fsscBaseBudgetItemComOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseBudgetItemCom"
        	action="/fssc/base/fssc_base_budget_item_com/fsscBaseBudgetItemCom.do">
        </oper>
        <!-- 预算方案 -->
        <oper
        	id="fsscBaseBudgetSchemeOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseBudgetScheme"
        	action="/fssc/base/fssc_base_budget_scheme/fsscBaseBudgetScheme.do">
        </oper>
        <!-- 现金流量项目 -->
        <oper
        	id="fsscBaseCashFlowOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseCashFlow"
        	action="/fssc/base/fssc_base_cash_flow/fsscBaseCashFlow.do">
        </oper>
        <!-- 公司 -->
        <oper
        	id="fsscBaseCompanyOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseCompany"
        	action="/fssc/base/fssc_base_company/fsscBaseCompany.do">
        </oper>
        <!-- 公司组 -->
        <oper
        	id="fsscBaseCompanyGroupOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseCompanyGroup"
        	action="/fssc/base/fssc_base_company_group/fsscBaseCompanyGroup.do">
        </oper>
        <!-- 成本中心 -->
        <oper
        	id="fsscBaseCostCenterOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseCostCenter"
        	action="/fssc/base/fssc_base_cost_center/fsscBaseCostCenter.do">
        </oper>
        <!-- 成本中心类型 -->
        <oper
        	id="fsscBaseCostTypeOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseCostType"
        	action="/fssc/base/fssc_base_cost_type/fsscBaseCostType.do">
        </oper>
        <!-- 货币 -->
        <oper
        	id="fsscBaseCurrencyOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.base.model.FsscBaseCurrency"
        	action="/fssc/base/fssc_base_currency/fsscBaseCurrency.do">
        </oper>
