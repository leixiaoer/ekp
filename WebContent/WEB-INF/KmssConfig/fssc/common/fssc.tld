		<tag-class>
			com.landray.kmss.fssc.common.taglib.FsscCommonModuleExistsTag
		</tag-class>
		<body-content>JSP</body-content>
		<description></description>
		<attribute>
			<name>path</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>tips</name>
		<tag-class>
			com.landray.kmss.fssc.common.taglib.FsscCommonTipsTag
		</tag-class>
		<body-content>JSP</body-content>
		<description></description>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[ID，页面唯一]]></description>
		</attribute>
		<attribute>
			<name>content</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[提示语]]></description>
		</attribute>
	</tag>
	
	<function>
		<name>showBudgetColomu</name>
		<function-class>com.landray.kmss.fssc.budget.taglib.FsscFunctions</function-class>
		<function-signature>
			java.lang.String showBudgetColomu(java.lang.String)
		</function-signature>
	</function>
</taglib>