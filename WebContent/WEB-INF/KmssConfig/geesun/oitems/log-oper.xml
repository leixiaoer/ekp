<?xml version="1.0" encoding="UTF-8"?>

<log-oper>
	<logset>
	
		<oper
            id="geesunOitemsManageOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.geesun.oitems.model.GeesunOitemsManage"
            action="/geesun/oitems/geesun_oitems_manage/geesunOitemsManage.do">
        </oper>
        
        <oper
            id="geesunOitemsListingOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.geesun.oitems.model.GeesunOitemsListing"
            action="/geesun/oitems/geesun_oitems_listing/geesunOitemsListing.do">
            <query 
                method="checkOitemsList|listChileren|stockCount"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
            <query 
                method="inCount|outCount|receiveCount"
                logPoint="Base_UrlParam;Action_Find"
                messageKey="button.list">
            </query>
            <query 
                method="recordList"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="geesun-oitems:geesunOitems.button.search">
            </query>
            <query 
                method="downloadExcel"
                logPoint="Base_UrlParam;downloadExcel">
            </query>
            <query 
                method="saveExcel"
                logPoint="Base_UrlParam;saveExcel"
                messageKey="button.import">
            </query>
            <query 
                method="importOitems"
                logPoint="Base_UrlParam;importOitems"
                messageKey="button.update">
            </query>
            <query 
                method="inCountExport|outCountExport|receiveCountExport"
                logPoint="Base_UrlParam;Action_Find"
                messageKey="button.export">
            </query>
            <query 
                method="stockCountExport"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.export">
            </query>
        </oper>
        
        <oper
            id="geesunOitemsWarehousingRecordOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.geesun.oitems.model.GeesunOitemsWarehousingRecord"
            action="/geesun/oitems/geesun_oitems_warehousing_record/geesunOitemsWarehousingRecord.do">
        </oper>
        
        <oper
            id="geesunOitemsWarehousingRecordJoinListOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.geesun.oitems.model.GeesunOitemsWarehousingRecordJoinList"
            action="/geesun/oitems/geesun_oitems_warehousing_record_joinlist/geesunOitemsWarehousingRecordJoinList.do">
            <query 
                method="listRecord"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
        </oper>
        
        <oper
            id="geesunOitemsBudgerApplicationOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.geesun.oitems.model.GeesunOitemsBudgerApplication"
            action="/geesun/oitems/geesun_oitems_budger_application/geesunOitemsBudgerApplication.do">
            <query 
                method="draftDelete"
                logPoint="Service_Delete"
                messageKey="button.delete">
            </query>
            <query 
                method="listForReceive"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
            <query 
                method="receive"
                logPoint="Base_UrlParam;receive"
                messageKey="geesun-oitems:geesunOitems.button.receive">
            </query>
            <query 
                method="print"
                logPoint="Base_UrlParam;Action_Find"
                messageKey="button.print">
            </query>
            <query 
                method="save"
                logPoint="Service_Update"
                messageKey="button.save">
            </query>
        </oper>
        
        <oper
            id="geesunOitemsBudgerApplicationIndexOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.geesun.oitems.model.GeesunOitemsBudgerApplication"
            action="/geesun/oitems/geesun_oitems_budger_application/geesunOitemsBudgerApplicationIndex.do">
        </oper>
        
        <oper
            id="geesunOitemsShoppingTrolleyOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.geesun.oitems.model.GeesunOitemsShoppingTrolley"
            action="/geesun/oitems/geesun_oitems_shopping_trolley/geesunOitemsShoppingTrolley.do">
            <query 
                method="addNumber"
                logPoint="Base_UrlParam;addNumber">
            </query>
            <!-- <query 
                method="addOitems"
                logPoint="Base_UrlParam;addOitems"
                messageKey="">
            </query>
            <query 
                method="cancle"
                logPoint="Service_Delete"
                messageKey="">
            </query> -->
            <query 
                method="deleteTrolley"
                logPoint="Base_UrlParam;deleteTrolley">
            </query>
            <query 
                method="listForApplication|listForFdListingId|loadTrolley"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
        </oper>
        
        <oper
            id="geesunOitemsTempletOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.geesun.oitems.model.GeesunOitemsTemplet"
            action="/geesun/oitems/geesun_oitems_templet/geesunOitemsTemplet.do">
            <query 
                method="getTemp"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
        </oper>
        
        <oper
            id="geesunOitemsMonthReportOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.geesun.oitems.model.GeesunOitemsMonthReport"
            action="/geesun/oitems/geesun_oitems_month_report/geesunOitemsMonthReport.do">
            <query 
                method="exportMonthReport"
                logPoint="Base_UrlParam;exportMonthReport">
            </query>
        </oper>
        
        <oper
            id="geesunOitemsReportListingOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.geesun.oitems.model.GeesunOitemsReportListing"
            action="/geesun/oitems/geesun_oitems_report_listing/geesunOitemsReportListing.do">
        </oper>
        
        <oper
            id="geesunOitemsCountListingService"
            extends="BaseOperLog"
            model="*"
            action="/sys/common/dataxml.jsp">
            <query
            	paramName="s_bean"
            	paramValue="GeesunOitemsCountListingService"
            	logPoint="Base_UrlParam;geesunOitemsCountListingService"
            	messageKey="geesun-oitems:geesunOitems.tree.reporting">
            </query>
        </oper>
	</logset>
</log-oper>
