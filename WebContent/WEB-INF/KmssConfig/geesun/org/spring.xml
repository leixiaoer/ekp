		<property
			name="modelName"
			value="com.landray.kmss.geesun.org.model.GeesunOrgEkp" />
	</bean>

	<bean
		id="geesunOrgEkpTarget"
		parent="KmssBaseTarget"
		class="com.landray.kmss.geesun.org.service.spring.GeesunOrgEkpServiceImp">
		<property
			name="baseDao">
			<ref
				local="geesunOrgEkpDao" />
		</property>
	</bean>
	<bean
		id="geesunOrgEkpService"
		parent="KmssBaseService">
		<property
			name="target">
			<ref
				local="geesunOrgEkpTarget" />
		</property>
	</bean>
	
