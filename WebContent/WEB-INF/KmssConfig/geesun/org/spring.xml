    </bean>
    <bean 
        id="geesunOrgPersonTarget" 
        class="com.landray.kmss.geesun.org.service.spring.GeesunOrgPersonServiceImp" 
        parent="KmssBaseTarget">
        <property 
            name="baseDao">
            <ref 
                bean="geesunOrgPersonDao"/>
        </property>
    </bean>
    <bean 
        id="geesunOrgPersonService" 
        parent="KmssBaseService">
        <property 
            name="target">
            <ref 
                bean="geesunOrgPersonTarget"/>
        </property>
    </bean>
    <bean 
        id="geesunOrgPostDao" 
        class="com.landray.kmss.geesun.org.dao.hibernate.GeesunOrgPostDaoImp" 
        parent="KmssAuthBaseDao">
        <property 
            name="modelName">
            <value>com.landray.kmss.geesun.org.model.GeesunOrgPost</value>
        </property>
    </bean>
    <bean 
        id="geesunOrgPostTarget" 
        class="com.landray.kmss.geesun.org.service.spring.GeesunOrgPostServiceImp" 
        parent="KmssBaseTarget">
        <property 
            name="baseDao">
            <ref 
                bean="geesunOrgPostDao"/>
        </property>
    </bean>
    <bean 
        id="geesunOrgPostService" 
        parent="KmssBaseService">
        <property 
            name="target">
            <ref 
                bean="geesunOrgPostTarget"/>
        </property>
    </bean>
	
	<bean 
        id="geesunOrgRecordDao" 
        class="com.landray.kmss.geesun.org.dao.hibernate.GeesunOrgRecordDaoImp" 
        parent="KmssAuthBaseDao">
        <property 
            name="modelName">
            <value>com.landray.kmss.geesun.org.model.GeesunOrgRecord</value>
        </property>
    </bean>
    <bean 
        id="geesunOrgRecordTarget" 
        class="com.landray.kmss.geesun.org.service.spring.GeesunOrgRecordServiceImp" 
        parent="KmssBaseTarget">
        <property 
            name="baseDao">
            <ref 
                bean="geesunOrgRecordDao"/>
        </property>
