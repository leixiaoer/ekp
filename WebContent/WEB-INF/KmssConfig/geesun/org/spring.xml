<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	
	<bean 
        id="geesunOrgOrganDao" 
        class="com.landray.kmss.geesun.org.dao.hibernate.GeesunOrgOrganDaoImp" 
        parent="KmssAuthBaseDao">
        <property 
            name="modelName">
            <value>com.landray.kmss.geesun.org.model.GeesunOrgOrgan</value>
        </property>
    </bean>
    <bean 
        id="geesunOrgOrganTarget" 
        class="com.landray.kmss.geesun.org.service.spring.GeesunOrgOrganServiceImp" 
        parent="KmssBaseTarget">
        <property 
            name="baseDao">
            <ref 
                bean="geesunOrgOrganDao"/>
        </property>
    </bean>
    <bean 
        id="geesunOrgOrganService" 
        parent="KmssBaseService">
        <property 
            name="target">
            <ref 
                bean="geesunOrgOrganTarget"/>
        </property>
    </bean>
    <bean 
        id="geesunOrgPersonDao" 
        class="com.landray.kmss.geesun.org.dao.hibernate.GeesunOrgPersonDaoImp" 
