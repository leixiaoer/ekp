<?xml version="1.0" encoding="UTF-8"?>
<configs 
    xsi:schemaLocation="http://www.example.org/design-config ../../design.xsd " 
    xmlns="http://www.example.org/design-config" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <module 
        urlPrefix="/geesun/base/" 
        messageKey="geesun-base:module.geesun.base" 
        defaultValidator="roleValidator(role=ROLE_GEESUNBASE_DEFAULT)">
        <roles>ROLE_GEESUNBASE_DEFAULT;ROLE_GEESUNBASE_SETTING;ROLE_GEESUNBASE_PAYING;ROLE_GEESUNBASE_FINANCEQUERY</roles>
        <request 
            path="tree.jsp*" 
            defaultValidator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
        <request 
            path="geesun_base_bxsj/geesunBaseBxsj.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.geesun.base.model.GeesunBaseBxsj" 
            defaultValidator="roleValidator(role=ROLE_GEESUNBASE_DEFAULT)">
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_GEESUNBASE_DEFAULT)"/>
            <query 
                queryString="method=listQuery|exportReportData" 
                validator="roleValidator(role=ROLE_GEESUNBASE_FINANCEQUERY)"/>
        </request>
        <request 
            path="geesun_base_account/geesunBaseAccount.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.geesun.base.model.GeesunBaseAccount" 
            defaultValidator="roleValidator(role=ROLE_GEESUNBASE_DEFAULT)">
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_GEESUNBASE_DEFAULT)"/>
        </request>
        <request 
            path="geesun_base_audit/geesunBaseAudit.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.geesun.base.model.GeesunBaseAudit" 
            defaultValidator="roleValidator(role=ROLE_GEESUNBASE_DEFAULT)">
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_GEESUNBASE_DEFAULT)"/>
        </request>
        <request 
            path="geesun_base_pay/geesunBasePay.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.geesun.base.model.GeesunBasePay" 
            defaultValidator="roleValidator(role=ROLE_GEESUNBASE_DEFAULT)">
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_GEESUNBASE_DEFAULT)"/>
        </request>
        <request 
            path="geesun_base_project/geesunBaseProject.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.geesun.base.model.GeesunBaseProject" 
            defaultValidator="roleValidator(role=ROLE_GEESUNBASE_DEFAULT)">
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_GEESUNBASE_DEFAULT)"/>
            <query 
                queryString="method=updateallUpdateAllDisabled" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="method=updateUpdateAllDisabled|openUpdateAllDisabled" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)&amp;[roleValidator(role=ROLE_GEESUNBASE_DEFAULT)]"/>
        </request>
    </module>
    <module 
        urlPrefix="/sys/transport/">
        <request 
            path="sys_transport_import/SysTransportImport*"/>
        <request 
            path="sys_transport_export/SysTransportExport*"/>
    </module>
    <module 
        urlPrefix="/sys/transport/">
        <request 
            path="sys_transport_import/SysTransportImport*">
            <query 
                queryString="fdModelName=com.landray.kmss.geesun.base.model.GeesunBaseBxsj" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="fdModelName=com.landray.kmss.geesun.base.model.GeesunBaseAccount" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="fdModelName=com.landray.kmss.geesun.base.model.GeesunBaseAudit" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="fdModelName=com.landray.kmss.geesun.base.model.GeesunBasePay" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="fdModelName=com.landray.kmss.geesun.base.model.GeesunBaseProject" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
        </request>
        <request 
            path="sys_transport_export/SysTransportExport*">
            <query 
                queryString="fdModelName=com.landray.kmss.geesun.base.model.GeesunBaseBxsj" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="fdModelName=com.landray.kmss.geesun.base.model.GeesunBaseAccount" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="fdModelName=com.landray.kmss.geesun.base.model.GeesunBaseAudit" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="fdModelName=com.landray.kmss.geesun.base.model.GeesunBasePay" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
            <query 
                queryString="fdModelName=com.landray.kmss.geesun.base.model.GeesunBaseProject" 
                validator="roleValidator(role=ROLE_GEESUNBASE_SETTING)"/>
        </request>
    </module>
    <homePage 
        messageKey="geesun-base:module.geesun.base" 
        urlPrefix="geesun/base" 
        url="/geesun/base/index.jsp" 
        config="/moduleindex_notopic.jsp?nav=/geesun/base/tree.jsp"/>
    <profileConfig 
        key="geesun/base" 
        messageKey="{geesun-base:module.geesun.base}" 
        url="/sys/profile/moduleindex.jsp?nav=/geesun/base/tree.jsp" 
        icon="km_comminfo" 
        type="fs" 
        description="{geesun-base:module.geesun.base.description}"/>
</configs>
