<?xml version="1.0" encoding="UTF-8" ?>

<log-oper>
	<logset>
		<oper
            id="kmInstitutionKnowledgeOperLog"
            desc="This is an example of a normal configuration"
            extends="BaseOperLog"
            model="com.landray.kmss.km.institution.model.KmInstitutionKnowledge"
            action="/km/institution/km_institution_knowledge/kmInstitutionKnowledge.do">
            <query
                method="docList"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="km-institution:kmInstitutionKnowledge.oper.docList">
            </query>
            <query
            	method="listOut"
            	logPoint="Base_UrlParam;Action_FindAll"
            	messageKey="km-institution:kmInstitutionKnowledge.oper.listOut">
            </query>
            <query
            	method="listAbolish"
            	logPoint="Base_UrlParam;Action_FindAll"
            	messageKey="km-institution:kmInstitutionKnowledge.oper.listAbolish">
            </query>
            <query
            	method="listChildren"
            	logPoint="Base_UrlParam;Action_FindAll"
            	messageKey="km-institution:kmInstitutionKnowledge.oper.listChildren">
            </query>
        </oper>
		<oper
            id="kmInstitutionKnowledgeIndexOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.institution.model.KmInstitutionKnowledge"
            action="/km/institution/km_institution_knowledge/kmInstitutionKnowledgeIndex.do">
            <query
                method="listChildren"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="km-institution:kmInstitutionKnowledge.oper.listChildren">
            </query>
        </oper>
        <oper
        	id="kmInstitutionKnowledgeAbolishOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.km.institution.model.KmInstitutionKnowledgeAbolish"
        	action="/km/institution/km_inst_knowledge_abolish/kmInstitutionKnowledgeAbolish.do">
        	<query
        		method="abolish"
        		logPoint="Base_UrlParam;abolish">
        	</query>
        </oper>
        <oper
        	id="kmInstitutionTemplateOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.km.institution.model.KmInstitutionTemplate"
        	action="/km/institution/km_institution_template/kmInstitutionTemplate.do">
        </oper>
        <oper
        	id="kmInstitutionKnowledgePortletOperLog"
        	extends="BaseOperLog"
        	model="*"
        	action="/sys/common/dataxml.jsp">
        	<query
            	paramName="s_bean"
            	paramValue="kmInstitutionKnowledgePortlet"
            	logPoint="Base_UrlParam;kmInstitutionKnowledgePortlet"
            	messageKey="km-institution:portlet.kmInstitution.newDoc">
            </query>
        </oper>
	</logset>
</log-oper>