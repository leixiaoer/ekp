<?xml version="1.0" encoding="UTF-8"?>

<log-oper>
	<logset>
	
		<oper
            id="kmPindagateDataOperLog"
            extends="BaseOperLog"
            model="*"
            action="/km/pindagate/km_pindagate_main/kmPindagateData.do">
            <query 
                method="listChildren|manageList"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
        </oper>
        
        <oper
            id="kmPindagateTemplateOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.pindagate.model.KmPindagateTemplate"
            action="/km/pindagate/km_pindagate_template/kmPindagateTemplate.do">
       		<query 
                method="clone"
                logPoint="Base_UrlParam;clone"
                messageKey="button.copy">
            </query>
            <query 
                method="listChildren"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
        </oper>
        
        <oper
            id="kmPindagateQuestionOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.pindagate.model.KmPindagateQuestion"
            action="/km/pindagate/km_pindagate_question/kmPindagateQuestion.do">
            <!-- <query 
                method="editAtt"
                logPoint="Base_UrlParam;Action_Find"
                messageKey="button.edit">
            </query> -->
            <query 
                method="viewAtt"
                logPoint="Base_UrlParam;Action_Find"
                messageKey="button.view">
            </query>
        </oper>
        
        <oper
            id="kmPindagateResponseOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.pindagate.model.KmPindagateResponse"
            action="/km/pindagate/km_pindagate_response/kmPindagateResponse.do">
            <query 
                method="resultsDetail"
                logPoint="Base_UrlParam;findResultsDetailPage"
                messageKey="km-pindagate:kmPindagateMain.toolControl.analysisResponses">
            </query>
            <query 
                method="exportResultExcel"
                logPoint="Base_UrlParam;exportResultExcel"
                messageKey="km-pindagate:kmPindagateMain.toolControl.export">
            </query>
        </oper>
        
        <oper
            id="kmPindagateMainOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.pindagate.model.KmPindagateMain"
            action="/km/pindagate/km_pindagate_main/kmPindagateMain.do">
            <query 
                method="copyAsTemplate|saveAsTemplate"
                logPoint="Service_Add"
                model="com.landray.kmss.km.pindagate.model.KmPindagateMainTemp"
                messageKey="km-pindagate:kmPindagateMain.copyAsTemplate">
            </query>
            <query 
                method="downloadTemplate"
                logPoint="Base_UrlParam;downloadTemplate"
                messageKey="km-pindagate:kmPindagateMain.import.template.download">
            </query>
            <query 
                method="pressAbsent|pressAbsentAll"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="km-pindagate:button.pressAbsent">
            </query>
            <query 
                method="resultsView"
                logPoint="Base_UrlParam;Action_Find"
                messageKey="button.view">
            </query>
            <query 
                method="importAddressData"
                logPoint="Base_UrlParam;addImportData"
                messageKey="button.import">
            </query>
            <query 
                method="add"
                logPoint="Base_UrlParam;copyPindagate">
            </query>
            <query 
                method="savedraft"
                logPoint="Service_Add"
                messageKey="button.savedraft">
            </query>
            <query 
                method="startIndagate|startIndagateAll"
                logPoint="Base_UrlParam;updateStartIndagate"
                messageKey="km-pindagate:button.startIndagate">
            </query>
            <query 
                method="stopIndagate|stopIndagateAll"
                logPoint="Base_UrlParam;updateStopIndagate"
                messageKey="km-pindagate:button.stopIndagate">
            </query>
            <query 
                method="updatedraft"
                logPoint="Service_Update"
                messageKey="button.update">
            </query>
            <query 
                method="updateIndagateEndTime"
                logPoint="Base_UrlParam;updateIndagateEndTime"
                messageKey="km-pindagate:button.updateIndagateEndTime">
            </query>
            <query 
                method="updateIndagateResultReaders"
                logPoint="Base_UrlParam;updateIndagateResultReaders"
                messageKey="km-pindagate:button.updateIndagateResultReaders">
            </query>
            <query 
                method="viewIndagatePersonInfo"
                logPoint="Base_UrlParam;getDataFromAll"
                messageKey="km-pindagate:kmPindagateMain.toolControl.participantsInfo">
            </query>
            <query 
                method="listChildren|manageList"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
            <query 
                method="exportResultExcel"
                logPoint="Base_UrlParam;Action_Find"
                messageKey="km-pindagate:kmPindagateMain.toolControl.export">
            </query>
        </oper>
        
        <oper
            id="kmPindagateMainTempOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.pindagate.model.KmPindagateMainTemp"
            action="/km/pindagate/km_pindagate_main_temp/kmPindagateMainTemp.do">
            <query 
                method="selectQuestionList"
                logPoint="Base_UrlParam;Action_Find"
                messageKey="button.view">
            </query>
            <query 
                method="listChildren"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
        </oper>
        
        <oper
            id="kmPindagateQuestionResOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.pindagate.model.KmPindagateQuestionRes"
            action="/km/pindagate/km_pindagate_question_res/kmPindagateQuestionRes.do">
        </oper>
        
		<oper
            id="kmPindagateConfig"
            extends="sysAppConfigOperLog"
            model="*"
            action="/km/pindagate/km_pindagate_config/kmPindagateConfig.do">
        </oper>
        
        <oper
            id="kmPindagateMainPortletOperLog"
            extends="BaseOperLog"
            model="*"
            action="/km/pindagate/km_pindagate_main/kmPindagateMainPortlet.do">
            <query 
                method="listPortlet"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
        </oper>
        
	</logset>
</log-oper>