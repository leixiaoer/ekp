		<item name="config">
			<param name="fieldIds" value="fdOtherUnits"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdotherunits/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdotherunits/fdOtherUnits"></param>
