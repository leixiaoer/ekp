<?xml version="1.0" encoding="UTF-8"?>
<plugin
	xmlns="http://www.example.org/plugin-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/plugin-config ../../../plugin.xsd ">
	<!-- 基本属性布局扩展 -->
	<extension point="com.landray.kmss.sys.xform.fieldlayout" model="com.landray.kmss.km.supervise.model.KmSuperviseMain">
		<!-- 督办标题 -->
	    <item name="config">
			<param name="fieldIds" value="docSubject"></param>
			<param name="required" value="true"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/docsubject/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/docsubject/docSubject"></param>
		</item>
		<!-- 督办内容 -->
		<item name="config">
			<param name="fieldIds" value="fdContent"></param>
			<param name="required" value="true"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdcontent/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdcontent/fdContent"></param>
		</item>
		<!-- 开始时间 -->
		<item name="config">
			<param name="fieldIds" value="fdStartTime"></param>
			<param name="required" value="true"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdstarttime/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdstarttime/fdStartTime"></param>
		</item>
		<!-- 完成时间 -->
		<item name="config">
			<param name="fieldIds" value="fdFinishTime"></param>
