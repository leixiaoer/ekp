		    <param name="jspParams" value="km/supervise/fieldlayout/fdfinishtime/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdfinishtime/fdFinishTime"></param>
		</item>
		<!-- 督办来源 -->
		<item name="config">
			<param name="fieldIds" value="fdModelId"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdmodelid/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdmodelid/fdModelId"></param>
		</item>
		<!-- 主办单位/部门-->
		<item name="config">
			<param name="fieldIds" value="fdSysUnit"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdsysunit/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdsysunit/fdSysUnit"></param>
		</item>
		
		<!-- <item name="config">
			<param name="fieldIds" value="fdUnit"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdunit/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdunit/fdUnit"></param>
		</item> -->
		<!-- 主办分管领导-->
		<item name="config">
			<param name="fieldIds" value="fdLeads"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdleads/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdleads/fdLeads"></param>
		</item>
		<!-- 主办单位负责人-->
		<item name="config">
			<param name="fieldIds" value="fdSponsor"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdsponsor/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdsponsor/fdSponsor"></param>
		</item>
