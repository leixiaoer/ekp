		<item name="config">
			<param name="fieldIds" value="fdLevel"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdlevel/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdlevel/fdLevel"></param>
		</item>
