			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdotherliaisons/fdOtherLiaisons"></param>
		</item>
		<!-- 协办责任人-->
		<item name="config">
			<param name="fieldIds" value="fdOtherSponsors"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdothersponsors/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdothersponsors/fdOtherSponsors"></param>
		</item>
		<!-- <item name="config">
			<param name="fieldIds" value="fdOUnits"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdounits/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdounits/fdOUnits"></param>
		</item> -->
		<!-- 立项时间-->
		<item name="config">
			<param name="fieldIds" value="fdApprovalTime"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdapprovaltime/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdapprovaltime/fdApprovalTime"></param>
		</item>
