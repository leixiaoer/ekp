		<item name="config">
			<param name="fieldIds" value="fdUrgency"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/fdurgency/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/fdurgency/fdUrgency"></param>
		</item>
		<!-- 督办编号 -->
		<item name="config">
			<param name="fieldIds" value="docNumber"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="km/supervise/fieldlayout/docnumber/param.jsp"></param>
			<param name="jspRunProfix" value="km/supervise/fieldlayout/docnumber/docNumber"></param>
		</item>
	</extension>
</plugin>