<?xml version="1.0" encoding="UTF-8"?>

<log-oper>
	<logset>
	
		<oper
            id="kmOitemsManageOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.oitems.model.KmOitemsManage"
            action="/km/oitems/km_oitems_manage/kmOitemsManage.do">
        </oper>
        
        <oper
            id="kmOitemsListingOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.oitems.model.KmOitemsListing"
            action="/km/oitems/km_oitems_listing/kmOitemsListing.do">
            <query 
                method="checkOitemsList|listChileren|stockCount"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
            <query 
                method="inCount|outCount|receiveCount"
                logPoint="Base_UrlParam;Action_Find"
                messageKey="button.list">
            </query>
            <query 
                method="recordList"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="km-oitems:kmOitems.button.search">
            </query>
            <query 
                method="downloadExcel"
                logPoint="Base_UrlParam;downloadExcel">
            </query>
            <query 
                method="saveExcel"
                logPoint="Base_UrlParam;saveExcel"
                messageKey="button.import">
            </query>
            <query 
                method="importOitems"
                logPoint="Base_UrlParam;importOitems"
                messageKey="button.update">
            </query>
            <query 
                method="inCountExport|outCountExport|receiveCountExport"
                logPoint="Base_UrlParam;Action_Find"
                messageKey="button.export">
            </query>
            <query 
                method="stockCountExport"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.export">
            </query>
        </oper>
        
        <oper
            id="kmOitemsWarehousingRecordOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.oitems.model.KmOitemsWarehousingRecord"
            action="/km/oitems/km_oitems_warehousing_record/kmOitemsWarehousingRecord.do">
        </oper>
        
        <oper
            id="kmOitemsWarehousingRecordJoinListOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.oitems.model.KmOitemsWarehousingRecordJoinList"
            action="/km/oitems/km_oitems_warehousing_record_joinlist/kmOitemsWarehousingRecordJoinList.do">
            <query 
                method="listRecord"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
        </oper>
        
        <oper
            id="kmOitemsBudgerApplicationOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.oitems.model.KmOitemsBudgerApplication"
            action="/km/oitems/km_oitems_budger_application/kmOitemsBudgerApplication.do">
            <query 
                method="draftDelete"
                logPoint="Service_Delete"
                messageKey="button.delete">
            </query>
            <query 
                method="listForReceive"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
            <query 
                method="receive"
                logPoint="Base_UrlParam;receive"
                messageKey="km-oitems:kmOitems.button.receive">
            </query>
            <query 
                method="print"
                logPoint="Base_UrlParam;Action_Find"
                messageKey="button.print">
            </query>
            <query 
                method="save"
                logPoint="Service_Update"
                messageKey="button.save">
            </query>
        </oper>
        
        <oper
            id="kmOitemsBudgerApplicationIndexOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.oitems.model.KmOitemsBudgerApplication"
            action="/km/oitems/km_oitems_budger_application/kmOitemsBudgerApplicationIndex.do">
        </oper>
        
        <oper
            id="kmOitemsShoppingTrolleyOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.oitems.model.KmOitemsShoppingTrolley"
            action="/km/oitems/km_oitems_shopping_trolley/kmOitemsShoppingTrolley.do">
            <query 
                method="addNumber"
                logPoint="Base_UrlParam;addNumber">
            </query>
            <!-- <query 
                method="addOitems"
                logPoint="Base_UrlParam;addOitems"
                messageKey="">
            </query>
            <query 
                method="cancle"
                logPoint="Service_Delete"
                messageKey="">
            </query> -->
            <query 
                method="deleteTrolley"
                logPoint="Base_UrlParam;deleteTrolley">
            </query>
            <query 
                method="listForApplication|listForFdListingId|loadTrolley"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
        </oper>
        
        <oper
            id="kmOitemsTempletOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.oitems.model.KmOitemsTemplet"
            action="/km/oitems/km_oitems_templet/kmOitemsTemplet.do">
            <query 
                method="getTemp"
                logPoint="Base_UrlParam;Action_FindAll"
                messageKey="button.list">
            </query>
        </oper>
        
        <oper
            id="kmOitemsMonthReportOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.oitems.model.KmOitemsMonthReport"
            action="/km/oitems/km_oitems_month_report/kmOitemsMonthReport.do">
            <query 
                method="exportMonthReport"
                logPoint="Base_UrlParam;exportMonthReport">
            </query>
        </oper>
        
        <oper
            id="kmOitemsReportListingOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.km.oitems.model.KmOitemsReportListing"
            action="/km/oitems/km_oitems_report_listing/kmOitemsReportListing.do">
        </oper>
        
        <oper
            id="kmOitemsCountListingService"
            extends="BaseOperLog"
            model="*"
            action="/sys/common/dataxml.jsp">
            <query
            	paramName="s_bean"
            	paramValue="KmOitemsCountListingService"
            	logPoint="Base_UrlParam;kmOitemsCountListingService"
            	messageKey="km-oitems:kmOitems.tree.reporting">
            </query>
        </oper>
	</logset>
</log-oper>