<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.geesun.org.model.GeesunOrgPost" 
        table="geesun_org_post">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_geesunOrgPost_docCreateTime"/>
        <property 
            name="fdPostId" 
            column="fd_post_id" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdIsDel" 
            column="fd_is_del" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdPostName" 
            column="fd_post_name" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="fdParentId" 
            column="fd_parent_id" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdPostNo" 
            column="fd_post_no" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdSuperPostId" 
            column="fd_super_post_id" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdSuperDeptId" 
            column="fd_super_dept_id" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdQidongDate" 
            column="fd_qidong_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdNewDate" 
            column="fd_new_date" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
