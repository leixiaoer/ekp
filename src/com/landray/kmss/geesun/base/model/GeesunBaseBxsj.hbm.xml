<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.geesun.base.model.GeesunBaseBxsj" 
        table="geesun_base_bxsj">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdTwoAccountName" 
            column="fd_two_account_name" 
            update="true" 
            insert="true" 
            length="400"/>
        <property 
            name="fdTwoAccountCode" 
            column="fd_two_account_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdRelateName" 
            column="fd_relate_name" 
            update="true" 
            insert="true" 
            length="400"/>
        <property 
            name="fdRelateCode" 
            column="fd_relate_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdRelateType" 
            column="fd_relate_type" 
            update="true" 
            insert="true" 
            length="30"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_geesunBaseBxsj_docCreateTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_geesunBaseBxsj_docAlterTime"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="fdFirstCharger" 
            table="geesun_base_bxsj_fst" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="fdSecondCharger" 
            table="geesun_base_bxsj_snd" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
