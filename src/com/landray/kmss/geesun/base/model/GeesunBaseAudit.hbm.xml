<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.geesun.base.model.GeesunBaseAudit" 
        table="geesun_base_audit">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_geesunBaseAudit_docCreateTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_geesunBaseAudit_docAlterTime"/>
        <property 
            name="fdAccountName" 
            column="fd_account_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdAccountCode" 
            column="fd_account_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="fdPersons" 
            table="geesun_base_audit_ry" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgPerson" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
