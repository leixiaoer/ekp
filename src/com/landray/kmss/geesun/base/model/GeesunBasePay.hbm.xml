<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.geesun.base.model.GeesunBasePay" 
        table="geesun_base_pay">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_geesunBasePay_docCreateTime"/>
        <property 
            name="fdYwDate" 
            column="fd_yw_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdPayDate" 
            column="fd_pay_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdPeriod" 
            column="fd_period" 
            update="true" 
            insert="true" 
            length="20"/>
        <property 
            name="fdPayBank1" 
            column="fd_pay_bank1" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdPayBank2" 
            column="fd_pay_bank2" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdPayBank3" 
            column="fd_pay_bank3" 
            update="true" 
            insert="true" 
            length="300"/>
        <property 
            name="fdAccountId" 
            column="fd_account_Id" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdAccountCode" 
            column="fd_account_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdAccountName" 
            column="fd_account_name" 
            update="true" 
            insert="true" 
            length="300"/>
        <property 
            name="fdDemo" 
            column="fd_demo" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdPayMoney" 
            column="fd_pay_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdOrderId" 
            column="fd_order_id" 
            update="true" 
            insert="true"/>
        <property 
            name="fdRemark" 
            column="fd_remark" 
            update="true" 
            insert="true" 
            length="500"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdReview" 
            column="fd_review_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
