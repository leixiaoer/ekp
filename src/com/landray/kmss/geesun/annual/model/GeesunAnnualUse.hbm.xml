<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.geesun.annual.model.GeesunAnnualUse" 
        table="geesun_annual_use">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docSubject" 
            column="doc_subject" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_geesunAnnualUse_docCreateTime"/>
        <property 
            name="fdType" 
            column="fd_type" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdModelId" 
            column="fd_model_id" 
            update="true" 
            insert="true" 
            length="36"/>
        <property 
            name="fdModelName" 
            column="fd_model_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdUse" 
            column="fd_use" 
            update="true" 
            insert="true"/>
        <property 
            name="fdBeginDate" 
            column="fd_begin_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdEndDate" 
            column="fd_end_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdBeginTime" 
            column="fd_begin_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdEndTime" 
            column="fd_end_time" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdAnnual" 
            column="fd_annual_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
