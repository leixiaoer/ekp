<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.geesun.annual.model.GeesunAnnualMain" 
        table="geesun_annual_main">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_geesunAnnualMain_docCreateTime"/>
        <property 
            name="fdOwnerNo" 
            column="fd_owner_no" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdTotal" 
            column="fd_total" 
            update="true" 
            insert="true"/>
        <property 
            name="fdRemain" 
            column="fd_remain" 
            update="true" 
            insert="true"/>
        <property 
            name="fdFrozen" 
            column="fd_frozen" 
            update="true" 
            insert="true"/>
        <property 
            name="fdUsed" 
            column="fd_used" 
            update="true" 
            insert="true"/>
        <property 
            name="docReadCount" 
            column="doc_read_count" 
            update="true" 
            insert="true"/>
        <property 
            name="fdLastTotal" 
            column="fd_last_total" 
            update="true" 
            insert="true"/>
        <property 
            name="fdLastRemain" 
            column="fd_last_remain" 
            update="true" 
            insert="true"/>
        <property 
            name="fdLastFrozen" 
            column="fd_last_frozen" 
            update="true" 
            insert="true"/>
        <property 
            name="fdLastUsed" 
            column="fd_last_used" 
            update="true" 
            insert="true"/>
        <property 
            name="fdNextResetDate" 
            column="fd_next_reset_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdLastResetDate" 
            column="fd_last_reset_date" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdOwner" 
            column="fd_owner_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docDept" 
            column="doc_dept_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
