<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.geesun.oitems.model.GeesunOitemsBudgerApplication"
		table="geesun_oitems_budger_application">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="docNumber"
			column="doc_number" 
            update="true" 
            insert="true" 
            length="100" />

		<property
			name="docSubject"
			column="doc_subject"
			update="true"
			insert="true"
			not-null="false"
			length="200" />
		<property
			name="fdReason"
			column="fd_reason"
			update="true"
			insert="true"
			not-null="false"
			length="2000" />
		<property
			name="fdDesc"
			column="fd_desc"
			update="true"
			insert="true"
			not-null="false"
			length="2000" />
		<property
			name="titleRegulation"
			column="fd_title_regulation"
			update="true"
			insert="true"
			not-null="false"
			length="3000" />
		<property
			name="fdTotalAmount"
			column="fd_total_amount"
			update="true"
			insert="true"
			not-null="false"
			length="100" />
		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="authReaderFlag"
			column="auth_reader_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<property
			name="authEditorFlag"
			column="auth_editor_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			not-null="false"
			length="2" />
		<property
			name="fdOutStatus"
			column="fd_out_status"
			update="true"
			insert="true"
			not-null="false"
			length="2" />

		<property
			name="fdOutTime"
			column="fd_out_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdType"
			column="fd_type"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdLastModifiedTime"
			column="fd_last_modified_time"
			update="true"
			insert="true"
			not-null="true" />
		<property
			name="docIsNewVersion"
			column="doc_is_new_version"
			update="true"
			insert="true"
			not-null="false" />
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
	    <!-- 创建者所属部门-->
		<many-to-one
			name="docDept"
			column="doc_dept_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdApplyor"
			column="fd_applicants_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdTemplate"
			column="fd_categroy_id"
			insert="true"
			update="true"
			not-null="false" />

		<bag
			name="authAllReaders"
			table="geesun_oitems_apply_areader"
			lazy="true">
			<key
				column="fd_oitems_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_all_reader_id" />
		</bag>


		<bag
			name="authAllEditors"
			table="geesun_oitems_apply_aeditor"
			lazy="true">
			<key
				column="fd_oitems_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_all_editor_id" />
		</bag>

		<bag
			name="authEditors"
			table="geesun_oitems_apply_editor"
			lazy="true">
			<key
				column="fd_oitems_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>


		<bag
			name="authOtherEditors"
			table="geesun_oitems_apply_oeditor"
			lazy="true">
			<key
				column="fd_oitems_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>


		<bag
			name="authReaders"
			table="geesun_oitems_apply_reader"
			lazy="true">
			<key
				column="fd_oitems_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>

		<bag
			name="authOtherReaders"
			table="geesun_oitems_apply_oreader"
			lazy="true">
			<key
				column="fd_oitems_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>

		<bag
			name="geesunOitemsShoppingTrolleyList"
			inverse="true"
			cascade="all-delete-orphan"
			lazy="true">
			<key
				column="fd_application_id" />
			<one-to-many
				class="com.landray.kmss.geesun.oitems.model.GeesunOitemsShoppingTrolley" />
		</bag>

	</class>
</hibernate-mapping>
