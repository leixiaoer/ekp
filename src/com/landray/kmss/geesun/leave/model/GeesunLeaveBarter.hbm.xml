<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.geesun.leave.model.GeesunLeaveBarter" 
        table="geesun_leave_barter">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docSubject" 
            column="doc_subject" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_geesunLeaveBarter_docCreateTime"/>
        <property 
            name="fdStartTime" 
            column="fd_start_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdEndTime" 
            column="fd_end_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdLeaveHour" 
            column="fd_leave_hour" 
            update="true" 
            insert="true"/>
        <property 
            name="authChangeReaderFlag" 
            column="auth_change_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authRBPFlag" 
            column="auth_rbp_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authReaderFlag" 
            column="auth_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authEditorFlag" 
            column="auth_editor_flag" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docDept" 
            column="doc_dept_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="authReaders" 
            table="geesun_leave_barter_reader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authOtherReaders" 
            table="geesun_leave_barter_oreader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllReaders" 
            table="geesun_leave_barter_areader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authOtherEditors" 
            table="geesun_leave_barter_oeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllEditors" 
            table="geesun_leave_barter_aeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
